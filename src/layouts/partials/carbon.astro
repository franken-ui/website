---
const { format = "cover" } = Astro.props;
---

{
  import.meta.env.CARBON ? (
    <script
      is:inline
      async
      type="text/javascript"
      src={`//cdn.carbonads.com/carbon.js?serve=CW7DPKQI&placement=franken-uidev&format=${format}`}
      id="_carbonads_js"
    />
  ) : (
    ""
  )
}
